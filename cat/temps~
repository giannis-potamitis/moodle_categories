// Get multiple records from a query and output all their fields
$result = $DB->get_records('mycat_category', array('mycatid'=>$mycatid));
foreach ($result as $object => $fields) {
	foreach($fields as $fieldname => $value) {
	  echo $fieldname . ": " . $value . '<br/>';
	}
	echo '<br/><br/>';
}

<?php

class block_markers extends block_base {
    public function init() {
        $this->title = get_string('markers', 'block_markers');
    }
    
    
    public function get_content() {
    	if ($this->content !== null) {
      	return $this->content;
    }
 
    $this->content         =  new stdClass;
    $this->content->text   = 'Marker Administration';
    $this->content->footer = 'Multiple Markers';
 
    return $this->content;
  }
} 
